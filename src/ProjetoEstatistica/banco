create database bd_parque;

use bd_parque;

create table tb_frequentador(
TB_FREQUENTADOR_ID int primary key auto_increment,
TB_FREQUENTADOR_SEXO varchar(16) NOT NULL,
TB_FREQUENTADOR_IDADE int NOT NULL,
TB_FREQUENTADOR_FREQUENCIA_SEMANAL int NOT NULL,
TB_FREQUENTADOR_ESTADO_CIVIL varchar(16) NOT NULL,
TB_FREQUENTADOR_MEIO_DE_TRANSPORTE varchar(16) NOT NULL,
TB_FREQUENTADOR_TEMPO_PERMANENCIA_MINUTOS int NOT NULL,
TB_FREQUENTADOR_RENDA_FAMILIAR float NOT NULL
);
